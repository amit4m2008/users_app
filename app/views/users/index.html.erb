<table id="userListing" class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th class="has-filter">Age</th>
      <th class="has-filter">Gender</th>
      <th class="has-filter">Country</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.email %></td>
        <td><%= user.age %></td>
        <td><%= user.gender %></td>
        <td><%= user.country_name %></td>
      </tr>
    <% end %>
  </tbody>
  <tfoot>
    <tr>
      <th>&nbsp;</th>
      <th>&nbsp;</th>
      <th>&nbsp;</th>
      <th>Age</th>
      <th>Gender</th>
      <th>Country</th>
    </tr>
  </tfoot>
</table>

<div class="pull-right">
  <%#= will_paginate @users %>
</div>